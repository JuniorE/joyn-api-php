{
    "name": "juniore/joyn-api-php",
    "description": "api client for joyn",
    "version": "0.0.1",
    "keywords": [
        "juniore",
        "joyn-api-php"
    ],
    "homepage": "https://github.com/juniore/joyn-api-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jan Junior Everaert",
            "email": "janjunior.everaert@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "composer/ca-bundle": "^1.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0||~6.0||~7.0||~8.0",
    },
    "autoload": {
        "psr-4": {
            "JuniorE\\JoynApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JuniorE\\JoynApiPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
